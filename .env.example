# TelemetryX Environment Variables
# Copy this file to .env and fill in your values
# cp .env.example .env

# Database
DATABASE_URL=postgres://telemetryx:password@localhost:5432/telemetryx

# Redis
REDIS_URL=redis://localhost:6379

# Rust logging level (trace, debug, info, warn, error)
RUST_LOG=info

# Python environment (development, staging, production)
PYTHON_ENV=development

# API server
API_HOST=0.0.0.0
API_PORT=8080

# gRPC (Rust <-> Python communication)
GRPC_PORT=50051
